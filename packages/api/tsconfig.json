{
	"compilerOptions": {
		"target": "ESNext",
		"module": "Node16",
		"moduleResolution": "Node16",
		"outDir": "./dist",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"sourceMap": true,
		"resolveJsonModule": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noImplicitReturns": true,
		"skipLibCheck": true,
		"rootDir": "./src",
		"baseUrl": ".",
		"paths": {
			"#api/*": ["./src/*"]
		}
	},
	"include": ["src/**/*.ts", "src/**/*.d.ts"]
}
